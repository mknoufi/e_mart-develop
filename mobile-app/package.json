{
  "name": "e-mart-mobile",
  "version": "1.0.0",
  "description": "E Mart Mobile App - React Native",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace EMartMobile.xcworkspace -scheme EMartMobile -configuration Release -destination generic/platform=iOS -archivePath EMartMobile.xcarchive archive",
    "pod-install": "cd ios && pod install",
    "clean": "cd android && ./gradlew clean && cd .. && rm -rf node_modules && npm install"
  },
